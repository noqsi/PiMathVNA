 CellLabel->"In[2]:=",ExpressionUUID->"952535ef-75b2-4500-a470-ee483fbf6870"],

Cell["\<\
Issue a command, yield complete response including the command echo at the \
start and the prompt at the end .\
\>", "Text",
 CellChangeTimes->{{3.8342261273033*^9, 
  3.83422617931851*^9}},ExpressionUUID->"9348cac2-b5d7-44a8-b8ac-\
617a534ff696"],

Cell[BoxData[
 RowBox[{
  RowBox[{"doCommand", "[", "cmd_", "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"flushJunk", "[", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"WriteString", "[", 
     RowBox[{"VNAtty", ",", 
      RowBox[{"cmd", "<>", "\"\<\\r\>\""}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"readResponse", "[", "]"}]}], ")"}]}]], "Input",
 CellChangeTimes->{{3.8341701450850286`*^9, 3.83417022234438*^9}, {
   3.834170255685198*^9, 3.83417027697621*^9}, {3.83417033198311*^9, 
   3.834170431452636*^9}, 3.8341711549314737`*^9, {3.834224239554966*^9, 
   3.834224260874745*^9}, {3.834224304288172*^9, 3.834224304774858*^9}, {
   3.8342338743388243`*^9, 3.834233884266828*^9}, {3.835196837370763*^9, 
   3.835196892341167*^9}},
 CellLabel->"In[3]:=",ExpressionUUID->"7f20e074-0d86-4421-9b84-97aa8d192301"],

Cell["\<\
Append input characters to a string containing the characters in the response \
that have already been read.\
\>", "Text",
 CellChangeTimes->{{3.834226241289363*^9, 3.834226246220574*^9}, {
  3.834226281520296*^9, 
  3.834226369042899*^9}},ExpressionUUID->"00b0e052-729f-4bcd-a3ba-\
f297502834c2"],

Cell[BoxData[
 RowBox[{
  RowBox[{"readPartial", "[", "front_", "]"}], ":=", 
  RowBox[{"front", "<>", 
   RowBox[{"FromCharacterCode", "[", 
    RowBox[{"DeviceReadBuffer", "[", "VNAdev", "]"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.834172068889557*^9, 3.834172145748311*^9}, {
  3.834224317766122*^9, 3.834224329051099*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"8e81fb43-37e3-47cb-8f6e-19a6dc9f81d4"],

Cell[TextData[{
 StyleBox["noPrompt", "NaturalLanguageInput"],
 " yields ",
 StyleBox["True", "NaturalLanguageInput"],
 " if string ",
 StyleBox["s", "NaturalLanguageInput"],
 " doesn\[CloseCurlyQuote]t end with a command prompt."
}], "Text",
 CellChangeTimes->{{3.834173544284873*^9, 3.834173556255126*^9}, {
  3.834173603244331*^9, 3.834173719412382*^9}, {3.834224546086649*^9, 
  3.834224557287553*^9}},ExpressionUUID->"68f67fd4-03b8-4308-84b8-\
ca835bc28de1"],

Cell[BoxData[
 RowBox[{
  RowBox[{"noPrompt", "[", "s_", "]"}], ":=", 
  RowBox[{"!", 
   RowBox[{"StringMatchQ", "[", 
    RowBox[{"s", ",", 
     RowBox[{"___", "~~", "\"\<\\r\\nch> \>\"", "~~", "EndOfString"}]}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{{3.834172631314995*^9, 3.8341727224333477`*^9}, 
   3.834172757863016*^9, 3.834172898316907*^9, {3.834173450574319*^9, 
   3.8341734655872393`*^9}},
 CellLabel->"In[5]:=",ExpressionUUID->"d12c86c0-5f8a-4fce-850a-6851e76954dd"],

Cell["\<\
Use readPartial to collect incoming data until a prompt appears.\
\>", "Text",
 CellChangeTimes->{{3.834171214809545*^9, 3.834171223899252*^9}, {
  3.834224365775092*^9, 
  3.83422443022405*^9}},ExpressionUUID->"48e367a8-f991-44d8-8407-\
3a4ec29f98f5"],

Cell[BoxData[
 RowBox[{
  RowBox[{"readResponse", "[", "]"}], ":=", 
  RowBox[{"NestWhile", "[", 
   RowBox[{"readPartial", ",", "\"\<\>\"", ",", "noPrompt"}], 
   "]"}]}]], "Input",
